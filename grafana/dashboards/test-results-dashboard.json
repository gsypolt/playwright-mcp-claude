{
  "dashboard": {
    "title": "Playwright Test Results",
    "tags": ["playwright", "testing", "qa"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Test Pass Rate (Last 30 Days)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT test_date, pass_rate FROM test_pass_rate_daily WHERE test_date >= CURRENT_DATE - INTERVAL '30 days' ORDER BY test_date",
            "format": "time_series"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Pass Rate"
          }
        ]
      },
      {
        "id": 2,
        "title": "Total Tests Executed",
        "type": "stat",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT COUNT(*) as value FROM test_results WHERE started_at >= CURRENT_DATE - INTERVAL '7 days'"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "auto"
        }
      },
      {
        "id": 3,
        "title": "Current Pass Rate",
        "type": "stat",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT ROUND(100.0 * SUM(CASE WHEN status = 'passed' THEN 1 ELSE 0 END) / NULLIF(COUNT(*), 0), 2) as value FROM test_results WHERE started_at >= CURRENT_DATE - INTERVAL '7 days'"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "unit": "percent"
        }
      },
      {
        "id": 4,
        "title": "Failed Tests (Last 7 Days)",
        "type": "stat",
        "gridPos": { "x": 12, "y": 4, "w": 6, "h": 4 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT COUNT(*) as value FROM test_results WHERE status = 'failed' AND started_at >= CURRENT_DATE - INTERVAL '7 days'"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 5 },
                { "color": "red", "value": 10 }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Flaky Tests",
        "type": "stat",
        "gridPos": { "x": 18, "y": 4, "w": 6, "h": 4 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT COUNT(*) as value FROM flaky_tests WHERE flake_rate > 10"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 3 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Test Duration Trends",
        "type": "graph",
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT DATE(started_at) as time, test_type, AVG(duration_ms) as avg_duration FROM test_results tr JOIN test_cases tc ON tr.test_case_id = tc.id WHERE started_at >= CURRENT_DATE - INTERVAL '30 days' GROUP BY DATE(started_at), test_type ORDER BY time",
            "format": "time_series"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Duration"
          }
        ]
      },
      {
        "id": 7,
        "title": "Test Types Distribution",
        "type": "piechart",
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT test_type, COUNT(*) as count FROM test_results tr JOIN test_cases tc ON tr.test_case_id = tc.id WHERE started_at >= CURRENT_DATE - INTERVAL '7 days' GROUP BY test_type"
          }
        ],
        "options": {
          "legend": { "displayMode": "table", "placement": "right" }
        }
      },
      {
        "id": 8,
        "title": "Slowest Tests (Top 10)",
        "type": "table",
        "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT title, file_path, avg_duration_ms, execution_count FROM slowest_tests LIMIT 10",
            "format": "table"
          }
        ]
      },
      {
        "id": 9,
        "title": "Most Flaky Tests (Top 10)",
        "type": "table",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT title, file_path, flake_rate, occurrences, last_detected FROM most_flaky_tests LIMIT 10",
            "format": "table"
          }
        ]
      },
      {
        "id": 10,
        "title": "Test Runs by Branch",
        "type": "bargauge",
        "gridPos": { "x": 0, "y": 24, "w": 12, "h": 6 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT branch_name, COUNT(*) as count FROM test_runs WHERE started_at >= CURRENT_DATE - INTERVAL '7 days' GROUP BY branch_name ORDER BY count DESC LIMIT 10"
          }
        ]
      },
      {
        "id": 11,
        "title": "Average Test Duration by Browser",
        "type": "bargauge",
        "gridPos": { "x": 12, "y": 24, "w": 12, "h": 6 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT tc.browser, AVG(tr.duration_ms) as avg_duration FROM test_results tr JOIN test_cases tc ON tr.test_case_id = tc.id WHERE tr.started_at >= CURRENT_DATE - INTERVAL '7 days' GROUP BY tc.browser"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 12,
        "title": "Failure Heatmap (by Hour)",
        "type": "heatmap",
        "gridPos": { "x": 0, "y": 30, "w": 24, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT DATE_TRUNC('hour', started_at) as time, COUNT(*) as value FROM test_results WHERE status = 'failed' AND started_at >= CURRENT_DATE - INTERVAL '7 days' GROUP BY DATE_TRUNC('hour', started_at) ORDER BY time",
            "format": "time_series"
          }
        ]
      },
      {
        "id": 13,
        "title": "Performance Metrics Trends",
        "type": "graph",
        "gridPos": { "x": 0, "y": 38, "w": 24, "h": 8 },
        "targets": [
          {
            "refId": "A",
            "rawSql": "SELECT tr.started_at as time, tm.metric_name, AVG(tm.metric_value) as value FROM test_metrics tm JOIN test_results tr ON tm.test_result_id = tr.id WHERE tr.started_at >= CURRENT_DATE - INTERVAL '30 days' GROUP BY DATE(tr.started_at), tm.metric_name ORDER BY time",
            "format": "time_series"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Metric Value"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "avg": true,
          "max": true
        }
      }
    ],
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
