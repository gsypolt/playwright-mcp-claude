# Application URLs
BASE_URL=http://localhost:3000
API_BASE_URL=http://localhost:3000/api

# Google OAuth Credentials
GOOGLE_EMAIL=your-google-email@gmail.com
GOOGLE_PASSWORD=your-google-password

# Microsoft OAuth Credentials
MICROSOFT_EMAIL=your-microsoft-email@outlook.com
MICROSOFT_PASSWORD=your-microsoft-password

# Generic Login Credentials
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=testpassword123

# Admin Credentials
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=adminpassword123

# API Keys
API_KEY=your-api-key-here

# Claude API (for AI-powered test generation)
CLAUDE_API_KEY=your-claude-api-key

# Performance Testing
PERFORMANCE_THRESHOLD_MS=3000

# Test Results Aggregation
# Options: 'database', 'testdino', 'json', 'none'
AGGREGATION_METHOD=none

# Database Configuration (for aggregation_method=database)
DB_TYPE=postgresql
DB_HOST=localhost
DB_PORT=5432
DB_NAME=playwright_results
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL=false

# TestDino Configuration (for aggregation_method=testdino)
TESTDINO_API_KEY=your-testdino-api-key
TESTDINO_PROJECT_ID=your-project-id
TESTDINO_ENDPOINT=https://api.testdino.com/v1/results

# JSON Export Configuration (for aggregation_method=json)
JSON_OUTPUT_PATH=./test-results/aggregated-results.json
JSON_INCLUDE_STDIO=false
JSON_INCLUDE_ATTACHMENTS=false

# CI/CD Metadata
PROJECT_NAME=playwright-mcp-claude
BRANCH_NAME=main
COMMIT_SHA=
CI_PROVIDER=local
BUILD_ID=
ENVIRONMENT=development
